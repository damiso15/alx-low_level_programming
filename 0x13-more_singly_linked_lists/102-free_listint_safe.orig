#include "lists.h"

/**
 * free_listint_safe - a function that frees a listint_t list
 * @head: the linked list
 * Return: the size of the list that was freeâ€™d
 */
size_t free_listint_safe(listint_t **h)
{
	listint_t *temp, *new, *hold = *h;
	size_t num1, num2;

	num1 = 0;
	while (*h)
	{
		num1++;
		temp = *h;
		*h = (*h)->next;
		free(temp);
		new = hold;

		num2 = 0;
		while (num2 < num1)
		{
			if (*h == new)
			{
				*h = NULL;
				return (num1);
			}
			new = new->next;
			num2++;
		}
	}
	return (num1);
}
